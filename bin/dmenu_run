#!/usr/bin/zsh

stuff='firefox
chromium-browser
intellij
subl
gedit
zzz
lock
poweroff
shutter
firefox -private
i3-sensible-terminal
nm-applet'

rename="i3-msg 'rename workspace to \"$(i3-msg -t get_workspaces | jq -r '.[] | select(.focused) | .name')\"'"

todo=$( (echo "$stuff"; echo "$rename"; dmenu_path) | dmenu "$@" )

eval "$todo &!"
